        "######## VERSION 0.1.0 ########\n",
        "# 对代码进行了重构 (待检验)\n",
        "\n",
        "import torch\n",
        "from qpth.qp import QPFunction\n",
        "\n",
        "def build_qp_matrices(U, V, S, T, tau, X, y, lambda_reg):\n",
        "    \"\"\"\n",
        "    构建 Q, p, G, h 矩阵。\n",
        "    \"\"\"\n",
        "    n, d = X.shape\n",
        "    L = U.shape[0]\n",
        "    H = S.shape[0]\n",
